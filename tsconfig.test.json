{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "#content/*": ["./content/*"],
      "#modules/*": ["./modules/*"],
      "#public/*": ["./public/*"],
      "#server/*": ["./server/*"],
      "#shared/*": ["./shared/*"],
      "#test/*": ["./test/*"],
      "#layers/*": ["./layers/*"],
      "#assets/*": ["./app/assets/*"],
      "#components/*": ["./app/components/*"],
      "#composables/*": ["./app/composables/*"],
      "#layouts/*": ["./app/layouts/*"],
      "#middleware/*": ["./app/middleware/*"],
      "#pages/*": ["./app/pages/*"],
      "#plugins/*": ["./app/plugins/*"],
      "#utils/*": ["./app/utils/*"],
      "#stores/*": ["./app/stores/*"],
    }
  },
  "include": [
    "env.d.ts",
    ".nuxt/nuxt.d.ts",
    "**/*",
  ],
}
